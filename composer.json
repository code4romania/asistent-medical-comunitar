{
    "name": "code4romania/asistent-medical-comunitar",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["code4romania", "laravel", "asistent", "medical", "comunitar"],
    "license": "MPL-2.0",
    "require": {
        "php": "^8.4",
        "aedart/athenaeum-antivirus": "^9.12",
        "alcea/cnp": "^3.0",
        "blade-ui-kit/blade-heroicons": "^2.6",
        "commitglobal/app-version": "^v1.0",
        "commitglobal/enum-utils": "^v1.0",
        "filament/filament": "^4.0",
        "filament/spatie-laravel-media-library-plugin": "^4.0",
        "guava/calendar": "^2.0",
        "guzzlehttp/guzzle": "^7.10",
        "jeffgreco13/filament-breezy": "^3.0",
        "kalnoy/nestedset": "^6.0",
        "laravel/framework": "^12.31",
        "laravel/sanctum": "^4.2",
        "laravel/tinker": "^2.10",
        "maatwebsite/excel": "^3.1",
        "sentry/sentry-laravel": "^4.16",
        "spatie/laravel-activitylog": "^4.10",
        "spatie/laravel-csp": "^3.18",
        "spatie/laravel-onboard": "^2.6",
        "spatie/once": "^3.1",
        "staudenmeir/belongs-to-through": "^2.16",
        "staudenmeir/laravel-merged-relations": "^1.9",
        "stevegrunwell/time-constants": "^1.2",
        "tpetry/laravel-query-expressions": "^1.5"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.16",
        "barryvdh/laravel-ide-helper": "^3.6",
        "fakerphp/faker": "^1.24",
        "friendsofphp/php-cs-fixer": "^3.87",
        "itsgoingd/clockwork": "^5.3",
        "larastan/larastan": "^3.7",
        "laravel/sail": "^1.46",
        "laravel/telescope": "^5.12",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.8",
        "pestphp/pest": "^4.1",
        "pestphp/pest-plugin-laravel": "^4.0",
        "pestphp/pest-plugin-livewire": "^4.0",
        "spatie/laravel-ignition": "^2.9"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "@php artisan filament:upgrade",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta",
            "@php artisan ide-helper:models --nowrite"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "analyse": [
            "./vendor/bin/phpstan analyse"
        ],
        "test": [
            "@php artisan test"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/telescope"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
